{
	// Place your snippets for python here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Single integer input": {
		"prefix": "ii",
		"body": [
			"int(input())",
			"$0"
		],
		"description": "Quick single int input."
	},

	"Single float input": {
		"prefix": "fi",
		"body": [
			"float(input())",
			"$0"
		],
		"description": "Quick single float input."
	},

	"Line integer input": {
		"prefix": "li",
		"body": [
			"[int(k) for k in input().split()]",
			"$0"
		],
		"description": "Quick line int input."
	},

	"Line float input": {
		"prefix": "lf",
		"body": [
			"[float(k) for k in input().split()]",
			"$0"
		],
		"description": "Quick line float input."
	},

	"I/O template": {
		"prefix": "io",
		"body": [
			"import sys",
			"sys.stdin=open('input.txt', 'r')",
			"sys.stdout=open('output.txt', 'w')",
			"",
			"$0"
		],
		"description": "I/O text files for test cases."
	},

	"Quick print": {
		"prefix": "pr",
		"body": [
			"print($0)"
		],
		"description": "Print fn on-the-go."
	},

	"For range template": {
		"prefix": "fr",
		"body": [
			"for i in range($0)"
		],
		"description": "Quick for range loop."
	}

}
	
